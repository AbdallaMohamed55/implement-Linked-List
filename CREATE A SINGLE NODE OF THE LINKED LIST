#include <iostream>
using namespace std;

struct Node {
    int data;      
    Node* next;    
};

int main() {
    
    Node* head = new Node;
    
    head->data = 10;   
    head->next = nullptr; 
    
    cout << "Data: " << head->data << endl;
    cout << "Next: " << head->next << endl;  // it should print 0 or NULL

    delete head;
    
    return 0;
}
