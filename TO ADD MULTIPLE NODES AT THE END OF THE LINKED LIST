#include <iostream>
using namespace std;

struct Node {
    int data;        
    Node* next;      
};

void insertAtEnd(Node*& head, int value) {
    Node* newNode = new Node;
    newNode->data = value;
    newNode->next = nullptr;

    if (head == nullptr) {
        head = newNode;  
    } else {
        Node* temp = head;
        while (temp->next != nullptr) {
            temp = temp->next;  
        }
        temp->next = newNode;  
    }
}


void display(Node* head) {
    Node* temp = head;
    while (temp != nullptr) {
        cout << temp->data << " ";
        temp = temp->next;
    }
    cout << endl;
}

int main() {
    Node* head = nullptr; // Start with empty list


    insertAtEnd(head, 10);
    insertAtEnd(head, 20);
    insertAtEnd(head, 30);
    insertAtEnd(head, 40);

    
    cout << "Linked List Elements: ";
    display(head);

    
    Node* temp = head;
    while (temp != nullptr) {
        Node* del = temp;
        temp = temp->next;
        delete del;
    }

    return 0;
}
